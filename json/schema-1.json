{
	"$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "https://example.org/coaches-schema.json",
    "type": "array",
   	"title": "Coaches",
    "minItems": 1,
    "items":{
        "type": "object",
        "title": "Coach",
        "properties": {
            "type": {
                "type": "string",
                "format": "iri"
            },
            "iri": {
                "type": "string",
                "format": "iri"
            },
            "name": {
                "type": "object",
                "required": [
                    "en"
                ],
                "properties": {
                    "en": {
                        "type": "string"
                    }
                }
            },
            "levelOfExperience": {
                "type": "number",
                "format": "integer"
            },
            "dateOfBirth": {
                "type": "string",
                "format": "date"
            },
            "teams": {
                "type": "array",
                "items": {
                    "type": "string",
                    "format": "iri"
                }
            }
        },
        "required": [
            "iri",
            "name",
            "levelOfExperience",
            "dateOfBirth",
            "teams"
        ]
    }
}
